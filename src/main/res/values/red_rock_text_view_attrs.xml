<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="RedRockTextView" tools:ignore="ResourceName">
        <!--<attr name="strings" format="reference" />-->
        <attr name="displayedStrings" format="reference" />
        <attr name="selectedColor" format="color" />
        <attr name="orientation" format="enum">
            <enum name="vertical" value="1" />
            <enum name="horizontal" value="0" />
        </attr>
        <attr name="offsetBetweenText" format="integer" />

        <attr name="RedRockTextViewStyle" format="reference" tools:ignore="ResourceName" />
    </declare-styleable>
</resources>